{
  "name": "poe-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public -p 54545 --no-clear",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile public/build/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile public/build/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@material/typography": "^13.0.0",
    "@mdi/js": "^6.6.95",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@smui-extra/autocomplete": "^6.0.0-beta.15",
    "@smui/card": "^6.0.0-beta.15",
    "@smui/data-table": "^6.0.0-beta.15",
    "@smui/icon-button": "^6.0.0-beta.15",
    "@smui/layout-grid": "^6.0.0-beta.15",
    "@smui/list": "^6.0.0-beta.15",
    "@smui/paper": "^6.0.0-beta.15",
    "@smui/tab": "^6.0.0-beta.15",
    "@smui/tab-bar": "^6.0.0-beta.15",
    "@smui/textfield": "^6.0.0-beta.15",
    "@smui/tooltip": "^6.0.0-beta.15",
    "@tsconfig/svelte": "^2.0.1",
    "@types/lodash": "^4.14.180",
    "@types/luxon": "^2.3.1",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "gh-pages": "^3.2.3",
    "lodash": "^4.17.21",
    "luxon": "^2.3.1",
    "rollup": "^2.3.4",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "smui-theme": "^6.0.0-beta.15",
    "svelte": "^3.0.0",
    "svelte-check": "^2.4.5",
    "svelte-material-ui": "^6.0.0-beta.15",
    "svelte-navigator": "^3.1.5",
    "svelte-preprocess": "^4.10.4",
    "tslib": "^2.0.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "sirv-cli": "^2.0.0"
  }
}
